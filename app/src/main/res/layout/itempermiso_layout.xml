<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tvCorreo"
        android:layout_width="145dp"
        android:layout_height="55dp"
        android:fontFamily="monospace"
        android:text="Correo"
        android:textAlignment="textStart"
        android:textSize="12sp"
        app:layout_constraintStart_toEndOf="@+id/tvFecha_Dato"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="correo" />

    <TextView
        android:id="@+id/tvFecha_Dato"
        android:layout_width="99dp"
        android:layout_height="55dp"
        android:fontFamily="monospace"
        android:text="Fecha"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/tvNivelPermiso_Dato"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="fecha" />

    <TextView
        android:id="@+id/tvIDPermiso_Dato"
        android:layout_width="38dp"
        android:layout_height="55dp"
        android:fontFamily="monospace"
        android:text="Id"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvIdCofrade_Dato"
        android:layout_width="59dp"
        android:layout_height="55dp"
        android:layout_marginStart="3dp"
        android:fontFamily="monospace"
        android:text="idCofrade"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/tvIDPermiso_Dato"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvNivelPermiso_Dato"
        android:layout_width="66dp"
        android:layout_height="55dp"
        android:layout_marginStart="1dp"
        android:fontFamily="monospace"
        android:text="NivelPermiso"
        android:textAlignment="center"
        app:layout_constraintStart_toEndOf="@+id/tvIdCofrade_Dato"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="nivelPermiso" />

</androidx.constraintlayout.widget.ConstraintLayout>