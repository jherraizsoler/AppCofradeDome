<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Activities.AltaCofradesActivity">

    <EditText
        android:id="@+id/etIdCofrade_AltaPermisosCofrades"
        android:layout_width="311dp"
        android:layout_height="64dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:hint="ID COFRADE"
        android:inputType="number|numberSigned"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etCorreoElectronico"
        android:layout_width="311dp"
        android:layout_height="64dp"
        android:layout_marginTop="288dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:hint="CORREO ELECTRONICO"
        android:inputType="text|textEmailAddress"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnAlta"
        android:layout_width="156dp"
        android:layout_height="55dp"
        android:layout_marginTop="376dp"
        android:text="Dar de alta"
        app:cornerRadius="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etNivelPermiso"
        android:layout_width="311dp"
        android:layout_height="64dp"
        android:layout_marginTop="196dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:hint="NIVEL PERMISO"
        android:inputType="number|numberSigned"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>