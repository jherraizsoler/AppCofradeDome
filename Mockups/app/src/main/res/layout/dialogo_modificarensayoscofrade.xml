<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/etEnsayosNormalesMaximos_dialogo"
        android:layout_width="233dp"
        android:layout_height="57dp"
        android:layout_marginTop="208dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="31dp"
        android:ems="10"
        android:hint="Ensayos Normales Maximos"
        android:inputType="text"
        android:textColor="#000000"
        android:textColorHint="#2F2F2F"
        app:layout_constraintBottom_toTopOf="@+id/etPrimerApellido_dialogo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvEnsayosNormalesMinimos_dialogo"
        android:layout_width="228dp"
        android:layout_height="25dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="28dp"
        android:text="Ensayos Normales Minimos:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/etEnsayoNormalesMinimos_dialogo"
        android:layout_width="235dp"
        android:layout_height="69dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="31dp"
        android:ems="10"
        android:hint="Ensayos Normales Minimos"
        android:inputType="number"
        android:textColor="#000000"
        android:textColorHint="#2F2F2F"
        app:layout_constraintBottom_toTopOf="@+id/etPrimerApellido_dialogo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/tvEnsayosNormalesMaximos_dialogo"
        android:layout_width="240dp"
        android:layout_height="24dp"
        android:layout_marginTop="184dp"
        android:layout_marginEnd="20dp"
        android:text="Ensayos Normales Maximos:"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_Ensayos_Cancelar"
        android:layout_width="80dp"
        android:layout_height="68dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="36dp"
        app:cornerRadius="5dp"
        app:icon="@drawable/baseline_cancel_presentation_24"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        app:iconSize="45dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/etEnsayosNormalesMaximos_dialogo" />

    <Button
        android:id="@+id/btn_Ensayos_Guardar"
        android:layout_width="83dp"
        android:layout_height="67dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="28dp"
        app:cornerRadius="5dp"
        app:icon="@drawable/baseline_save_24"
        app:iconGravity="textStart"
        app:iconPadding="0dp"
        app:iconSize="45dp"
        app:layout_constraintEnd_toStartOf="@+id/btn_Ensayos_Cancelar"
        app:layout_constraintTop_toBottomOf="@+id/etEnsayosNormalesMaximos_dialogo" />

</androidx.constraintlayout.widget.ConstraintLayout>